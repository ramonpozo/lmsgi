<xs:element name="urlset">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="url" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="loc" type="anyURI"/>
						<xs:element name="lastmod" type="date"/>
						<xs:element name="priority" type="xs:float"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:element>